<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="9.0.0" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="GeneralErrorResource.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>GeneralErrorResource.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="PlatformTypeResource.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>GeneralErrorResource.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="WealthTypeResource.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>WealthTypeResource.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="RegionResource.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>RegionResource.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="GeneralErrorResource.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>GeneralErrorResource.resx</DependentUpon>
      </Compile>
      <Compile Update="PlatformTypeResource.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>PlatformTypeResource.resx</DependentUpon>
      </Compile>
      <Compile Update="WealthTypeResource.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>WealthTypeResource.resx</DependentUpon>
      </Compile>
      <Compile Update="RegionResource.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>RegionResource.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
