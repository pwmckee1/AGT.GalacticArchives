{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "AllowedOrigins": [
      "https://localhost:4200"
    ],
    "Firebase": {
      "CredentialsPath": "firebase-credentials.json",
      "ProjectId": "agt-galactic-archives-b9a30",
      "SecretsId": "firebase-credentials"
    },
    "EnableDeveloperErrors": true,
    "GoogleCloudProjectId": "agt-galactic-archives-b9a30",
    "NLog": {
      "throwConfigExceptions": true,
      "targets": {
        "async": true,
        "logconsole": {
          "type": "ColoredConsole"
        }
      },
      "rules": [
        {
          "logger": "*",
          "minLevel": "Debug",
          "writeTo": "logconsole"
        }
      ]
    }
  }
}
